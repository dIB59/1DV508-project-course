<?xml version="1.0" encoding="UTF-8"?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane xmlns="http://javafx.com/javafx"
            xmlns:fx="http://javafx.com/fxml"
            fx:controller="org.example.features.feedback.FeedbackController"
            prefHeight="400.0" prefWidth="600.0">

    <VBox alignment="CENTER"
          layoutX="150.0"
          layoutY="100.0"
          spacing="15.0"
          AnchorPane.leftAnchor="0"
          AnchorPane.rightAnchor="0"
          AnchorPane.topAnchor="0"
          AnchorPane.bottomAnchor="0">

        <Label fx:id=""
               text="Please select one of the ratings bellow"
               style="-fx-text-fill: black; -fx-font-size: 28px; -fx-font-weight: bold;"/>

        <Button text="1" onAction="#handleRating"/>
        <Button text="2" onAction="#handleRating"/>
        <Button text="3" onAction="#handleRating"/>
        <Button text="4" onAction="#handleRating"/>
        <Button text="5" onAction="#handleRating"/>

    </VBox>
</AnchorPane>
